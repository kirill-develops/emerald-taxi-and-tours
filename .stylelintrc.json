{
   "extends": [
      "stylelint-config-standard"
   ],
   "overrides": [
      {
         "files": [
            "**/*.jsx"
         ],
         "customSyntax": "@stylelint/postcss-css-in-js"
      }
   ],
   "rules": {
      "function-no-unknown": [
         true,
         {
            "ignoreFunctions": [
               "/\\${/"
            ]
         }
      ]
   }
}